{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Vidau",
  "description": "Vidau - AI Media Generation Platform",
  "colors": {
    "primary": "#3459FF",
    "light": "#3452FF",
    "dark": "#425DFF"
  },
  "navigation": {
    "groups": [
      {
        "group": "Overview",
        "pages": [
          "overview/introduction",
          "overview/pricing",
          "overview/models"
        ]
      },
      {
        "group": "API Documentation",
        "pages": [
          "api/get-api-key",
          {
            "group": "Video Task APIs",
            "openapi": {
              "source": "openapi.json",
              "tag": "video"
            }
          },
          {
            "group": "Image Task APIs",
            "openapi": {
              "source": "openapi.json",
              "tag": "image"
            }
          },
          "api/changelog"
        ]
      },
      {
        "group": "Terms & Agreements",
        "pages": [
          "terms/terms-and-conditions"
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": [
        "javascript",
        "curl",
        "python",
        "java",
        "go"
      ]
    }
  },
  "logo": {
    "light": "VidauLogo.png",
    "dark": "VidauLogo.png",
    "href": "https://www.vidau.ai"
  },
  "navbar": {
    "links": []
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/vidau",
      "linkedin": "https://linkedin.com/company/vidau",
      "github": "https://github.com/vidau",
      "discord": "https://discord.gg/vidau"
    },
    "links": [
      {
        "header": "Product",
        "items": [
          {
            "label": "Features",
            "href": "https://www.vidau.ai/features"
          },
          {
            "label": "Pricing",
            "href": "https://www.vidau.ai/pricing"
          },
          {
            "label": "Use Cases",
            "href": "https://www.vidau.ai/cases"
          }
        ]
      },
      {
        "header": "Resources",
        "items": [
          {
            "label": "Documentation",
            "href": "https://docs.vidau.ai"
          },
          {
            "label": "Blog",
            "href": "https://blog.vidau.ai"
          },
          {
            "label": "Community",
            "href": "https://community.vidau.ai"
          }
        ]
      },
      {
        "header": "Company",
        "items": [
          {
            "label": "About Us",
            "href": "https://www.vidau.ai/about"
          },
          {
            "label": "Contact",
            "href": "https://www.vidau.ai/contact"
          },
          {
            "label": "Privacy Policy",
            "href": "https://www.vidau.ai/privacy-policy/"
          }
        ]
      },
      {
        "header": "Support",
        "items": [
          {
            "label": "Help Center",
            "href": "https://help.vidau.ai"
          },
          {
            "label": "Status",
            "href": "https://status.vidau.ai"
          },
          {
            "label": "Submit Ticket",
            "href": "https://support.vidau.ai"
          }
        ]
      }
    ]
  },
  "integrations": {
    "ga4": {
      "measurementId": "G-XXXXXXXXXX"
    },
    "telemetry": {
      "enabled": true
    },
    "cookies": {
      "key": "vidau_cookie_key",
      "value": "vidau_cookie_value"
    }
  },
  "contextual": {
    "options": [
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "features": {
    "search": true,
    "darkMode": true,
    "tableOfContents": true,
    "breadcrumbs": true
  },
  "sidebar": {
    "collapsible": true,
    "autoCollapse": false
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "Page Not Found",
      "description": "Sorry, we couldn't find the page you're looking for. Please check the URL or return to the [homepage](/)"
    }
  }
}