{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Vidau",
  "description": "Vidau - AI媒体生成平台",
  "colors": {
    "primary": "#3B82F6",
    "light": "#F8FAFC",
    "dark": "#0F172A"
  },
  "navigation": {
      "groups": [
          {
            "group": "快速开始",
            "pages": [
              "getting-started"
            ]
          },
           {
            "group": "接口端点",
            "openapi": {
              "source": "openapi.json"
            }
          }
        ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": [
        "javascript",
        "curl",
        "python"
      ]
    }
  },
  "logo": {
    "light": "VidauLogo.png",
    "dark": "VidauLogo.png",
    "href": "https://www.vidau.ai"
  },
  "navbar": {
    "links": [],
    "primary": {
      "type": "button",
      "label": "控制台",
      "href": "https://console.vidau.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/vidau",
      "linkedin": "https://linkedin.com/company/vidau",
      "github": "https://github.com/vidau",
      "discord": "https://discord.gg/vidau"
    },
    "links": [
      {
        "header": "产品",
        "items": [
          {
            "label": "功能介绍",
            "href": "https://www.vidau.ai/features"
          },
          {
            "label": "定价",
            "href": "https://www.vidau.ai/pricing"
          },
          {
            "label": "使用案例",
            "href": "https://www.vidau.ai/cases"
          }
        ]
      },
      {
        "header": "资源",
        "items": [
          {
            "label": "文档",
            "href": "https://docs.vidau.ai"
          },
          {
            "label": "博客",
            "href": "https://blog.vidau.ai"
          },
          {
            "label": "社区",
            "href": "https://community.vidau.ai"
          }
        ]
      },
      {
        "header": "公司",
        "items": [
          {
            "label": "关于我们",
            "href": "https://www.vidau.ai/about"
          },
          {
            "label": "联系我们",
            "href": "https://www.vidau.ai/contact"
          },
          {
            "label": "隐私政策",
            "href": "https://www.vidau.ai/privacy-policy/"
          }
        ]
      },
      {
        "header": "支持",
        "items": [
          {
            "label": "帮助中心",
            "href": "https://help.vidau.ai"
          },
          {
            "label": "状态页面",
            "href": "https://status.vidau.ai"
          },
          {
            "label": "提交工单",
            "href": "https://support.vidau.ai"
          }
        ]
      }
    ]
  },
  "integrations": {
    "ga4": {
      "measurementId": "G-XXXXXXXXXX"
    },
    "telemetry": {
      "enabled": true
    },
    "cookies": {
      "key": "vidau_cookie_key",
      "value": "vidau_cookie_value"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "features": {
    "search": true,
    "darkMode": true,
    "tableOfContents": true,
    "breadcrumbs": true
  },
  "sidebar": {
    "collapsible": true,
    "autoCollapse": false
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "页面未找到",
      "description": "抱歉，我们找不到您要查找的页面。请检查URL或返回[首页](/)"
    }
  }
}