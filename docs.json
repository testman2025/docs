{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Vidau",
  "description": "Vidau - AI媒体生成平台",
  "colors": {
    "primary": "#3B82F6",
    "light": "#F8FAFC",
    "dark": "#0F172A"
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "简介",
        "icon": "info",
        "description": "Vidau平台介绍",
        "pages": [
          "index"
        ]
      },
      {
        "dropdown": "API",
        "icon": "code",
        "description": "完整的API参考文档",
        "groups": [
          {
            "group": "快速开始",
            "pages": [
              "api/getting-started",
              "api/authentication"
            ]
          },
          {
            "group": "核心接口",
            "pages": [
              "api/images",
              "api/videos",
              "api/models"
            ]
          },
          {
            "group": "接口端点",
            "openapi": {
              "source": "openapi.json"
            }
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": [
        "javascript",
        "curl",
        "python"
      ]
    }
  },
  "logo": {
    "light": "/video-remix/logo1.svg",
    "dark": "/video-remix/logo1.svg",
    "href": "https://static.vidau.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "社区",
        "href": "https://www.vidau.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "立即开始",
      "href": "https://www.vidau.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://www.vidau.ai",
      "linkedin": "https://www.vidau.ai",
      "github": "https://www.vidau.ai",
      "slack": "https://www.vidau.ai"
    },
    "links": [
      {
        "header": "资源",
        "items": [
          {
            "label": "客户案例",
            "href": "https://www.vidau.ai"
          },
          {
            "label": "企业版",
            "href": "https://www.vidau.ai"
          },
          {
            "label": "申请预览",
            "href": "https://www.vidau.ai"
          }
        ]
      },
      {
        "header": "公司",
        "items": [
          {
            "label": "招聘信息",
            "href": "https://www.vidau.ai"
          },
          {
            "label": "博客",
            "href": "https://www.vidau.ai"
          },
          {
            "label": "隐私政策",
            "href": "https://www.vidau.ai/privacy-policy/"
          }
        ]
      }
    ]
  },
  "integrations": {
    "ga4": {
      "measurementId": "G-XXXXXXXXXX"
    },
    "telemetry": {
      "enabled": true
    },
    "cookies": {
      "key": "example_cookie_key",
      "value": "example_cookie_value"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "页面未找到",
      "description": "这个页面**到底**_去哪了_？"
    }
  }
}